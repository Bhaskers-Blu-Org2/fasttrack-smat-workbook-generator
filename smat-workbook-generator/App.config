<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>

    <supportedRuntime version="v2.0.50727"/>
  </startup>

  <system.diagnostics>
    <!-- Configure any trace settings required here. By default we write to a log file -->
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="smatwbListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="smatwb.log" />
        <remove name="Default" />
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>
